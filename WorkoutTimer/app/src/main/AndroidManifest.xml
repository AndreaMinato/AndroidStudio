<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.andrea.workouttimer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".StartWorkoutActivity">
        </activity>
        <activity android:name=".WorkoutActivity">
        </activity>
        <activity android:name=".WorkoutListActivity">
        </activity>
        <activity android:name=".WorkoutDetailActivity">
        </activity>

        <provider
            android:name=".database.WorkoutContentProvider"
            android:authorities="com.minatoandrea_WorkoutContentProvider"
            android:exported="false" />

    </application>

</manifest>